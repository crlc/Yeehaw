<blockquote>
  <header><strong><%= post.get("handle") %></strong></header>
  <p><%= post.escape("body") %></p>
  <footer><span class="glyphicon glyphicon-time"></span> <%= post.get("time_ago")%> ago <%= post.get("reply_count") %> replies</footer>
  <button class="del" name="destroy">Delete</button>
</blockquote>
